  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "babel-node src/main.ts",
    "start:dev": "nodemon -e js,hbs hbs-preview",
